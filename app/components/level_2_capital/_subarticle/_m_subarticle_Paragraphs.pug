mixin subarticle_Paragraphs(paragraphsTotal)
  - for (let paragraphCounter = 1; paragraphCounter <= paragraphsTotal; paragraphCounter++)
    -
      const paragraphText =
        _TEMPLATE_PARAGRAPHS[(paragraphCounter + 1) % 2];
    p.generic-textblock.subarticle__paragraph(class=(
      paragraphCounter === paragraphsTotal
      ? 'subarticle__paragraph--last'
      : null
    ))= paragraphText
